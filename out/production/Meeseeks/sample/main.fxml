<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="site" prefHeight="600.0" prefWidth="800.0" stylesheets="@../TextTests/StyleSheet.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <ImageView fx:id="secTile" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/Banner.JPG" />
         </image>
      </ImageView>
      <Label fx:id="textoArchivo" layoutX="110.0" layoutY="173.0" text="Archivos: ">
         <font>
            <Font name="Bodoni MT" size="19.0" />
         </font>
      </Label>
      <Button fx:id="myButton" layoutX="311.0" layoutY="174.0" mnemonicParsing="false" onAction="#buttonPressed" prefHeight="22.0" prefWidth="133.0" text="Elija archivo" />
      <ComboBox fx:id="myComboBox" layoutX="311.0" layoutY="116.0" nodeOrientation="LEFT_TO_RIGHT" prefWidth="150.0" promptText="Empresas" />
      <Label fx:id="textoSeleccione" layoutX="51.0" layoutY="114.0" text="Seleccione la empresa: ">
         <font>
            <Font name="Bodoni MT" size="21.0" />
         </font>
      </Label>
      <TableView fx:id="myView" layoutX="66.0" layoutY="365.0" prefHeight="188.0" prefWidth="688.0" />
      <Label fx:id="textoErrores" layoutX="96.0" layoutY="322.0" prefHeight="27.0" prefWidth="181.0" text="Resultados tabla de errores:" />
   </children>
</Pane>
